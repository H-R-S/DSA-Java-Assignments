public class InsertionSort {
Public void sort(int arr[])  {
        int n = arr.length;
        int i = 1; 
        while (i < n) {
            int key = arr[i];
            int j = i - 1;
            while (j >= 0 && arr[j] > key) {
                arr[j + 1] = arr[j];
                j = j - 1;     }
            arr[j + 1] = key;
            i++;   }  }
    static void printArray(int arr[])   {
        int n = arr.length;
        for (int i = 0; i < n; ++i)
            System.out.print(arr[i] + " ");
   System.out.println();    }
    public static void main(String args[]) {
        int arr[] = { 34, 67, 5, 12, 6, 72, 1, 29 };
 InsertionSort is = new InsertionSort();
        is.sort(arr);
     printArray(arr); }  }
